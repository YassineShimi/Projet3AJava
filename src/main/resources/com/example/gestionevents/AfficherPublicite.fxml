            <VBox fx:id="publiciteVbox" prefHeight="482.0" prefWidth="347.0" styleClass="hBox_main" stylesheets="@style.css" />
         </content>
      </ScrollPane>
      <AnchorPane layoutX="317.0" layoutY="50.0" prefHeight="373.0" prefWidth="275.0">
         <children>
            <TextField fx:id="descPublicite" layoutX="14.0" layoutY="46.0" />
            <TextField fx:id="sponsorPublicite" layoutX="23.0" layoutY="129.0" />
            <TextField fx:id="typePublicite" layoutX="24.0" layoutY="88.0" />
            <Button fx:id="updatePubliciteBtn" layoutX="194.0" layoutY="46.0" mnemonicParsing="false" onAction="#OnCllickedUpdatePublicite" styleClass="btnQuit" stylesheets="@style.css" text="update" />
            <Button fx:id="suppPublicite" layoutX="195.0" layoutY="88.0" mnemonicParsing="false" onAction="#OnClickedSuppPublicite" styleClass="btnQuit" stylesheets="@style.css" text="supprimer" />
