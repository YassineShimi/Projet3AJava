<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="normalBtn" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gestionevents.controllers.AfficherPubliciteController">
   <children>
      <ScrollPane layoutX="30.0" layoutY="37.0" prefHeight="400.0" prefWidth="275.0" styleClass="pane_top" stylesheets="@style.css">
